<IfModule mod_negotiation.c>
   Options -MultiViews
</IfModule>
 
<IfModule mod_rewrite.c>
#  Options -Multiviews
  RewriteEngine On
  
  #path should be small letter
  RewriteBase /mvcoop/public    
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteRule  ^(.+)$ index.php?url=$1 [QSA,L]
</IfModule>